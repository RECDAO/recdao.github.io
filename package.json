    "promise-log": "^0.1.1",
    "promise-polyfill": "^6.0.2",
    "truffle-hdwallet-provider": "0.0.3",
    "vue": "^2.2.0",
    "vue-router": "^2.0.0",
    "vuetify": "^1.0.0",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^4.0.0",
    "web3": "^1.0.0-beta.30"
