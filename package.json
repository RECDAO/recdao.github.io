{
  "name": "recdao",
  "version": "0.1.0",
  "description": "Reddit/Ethereum Community DAO",
  "main": "client/index.js",
  "scripts": {
    "test": "",
    "build": "webpack --config build/webpack.prod.js",
    "dev": "node build/server.js",
    "embark-rinkeby": "NODE_ENV=production embark run rinkeby --nodashboard",
    "rinkeby-node": "geth --rpc --rinkeby --syncmode 'fast' --unlock '0x80f66D2E0eaFa73910ED0B5d411640b48d23B838'",
    "web-deploy": "git subtree push --prefix web origin master"
  },
  "author": "Carl Larson <cslarson@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ethereum",
    "ethtrader",
    "solidity",
    "reddit"
  ],
  "xo": {
    "extends": "./.eslintrc",
    "esnext": true,
    "envs": [
      "browser"
    ]
  },
  "babel": {
    "presets": [
      [
        "vue-app",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-nested": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie > 8"
  ],
  "dependencies": {
    "array-unique": "^0.3.2",
    "async": "^2.6.0",
    "babel-runtime": "^6.18.0",
    "bases": "^0.2.1",
